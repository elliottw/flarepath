(page "index.html")

(defc text-input "")
(html
  (head
    (link :href "app.css" :rel "stylesheet" :type "text/css"))
  (body
     (input
      :type "text"
      :placeholder "Type something here"
      :value text-input
      :keyup #(reset! text-input @%))
     (p (text "Value of text input: ~{text-input}"))))
